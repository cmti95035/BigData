<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
  http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
  version="1.0">

<!-- 
cd src\main\resources
jar -cvf p.jar META-INF
 -->                  
    <persistence-unit name="DASHBOARD" transaction-type="RESOURCE_LOCAL">
    	<!-- provider>org.hibernate.ejb.HibernatePersistence</provider -->
    	<provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
		<class>com.cmti.analytics.app.station.olapdatabase.domain.DimStation</class>
		<class>com.cmti.analytics.app.station.olapdatabase.domain.FactImsi</class>
		<class>com.cmti.analytics.app.station.olapdatabase.domain.FactEvent</class>	
		<class>com.cmti.analytics.app.station.olapdatabase.domain.SigHistory</class>	
		
		
		<exclude-unlisted-classes>true</exclude-unlisted-classes>	
      	<properties>
        	<property name="hibernate.cache.use_second_level_cache" value="false"/>
         	<property name="hibernate.hbm2ddl.auto" value="update"/> 
      	</properties>	
    </persistence-unit>
               
</persistence>
